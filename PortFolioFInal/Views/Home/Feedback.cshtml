@model List<ContactModel> // Assuming your feedbackData is a list of ContactModel

<h2>Feedback</h2>

@if (Model.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Message</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                    <td>@item.Email</td>
                    <td>@item.Message</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No feedback available.</p>
}
